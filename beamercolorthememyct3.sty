\setbeamertemplate{headline}{%
\leavevmode%
  \hbox{%
    \begin{beamercolorbox}[wd=\paperwidth,ht=2.5ex,dp=1.125ex]{headline}%
%    \insertsectionnavigationhorizontal{\paperwidth}{\bfseries\hfill}{\hfill}
    \insertsubsectionnavigationhorizontal{\paperwidth}{\bfseries\hfill}{\hfill}
    \end{beamercolorbox}%
  }
}
\setbeamertemplate{footline}{%
\leavevmode%
  \hbox{%
    \setbeamercolor{footlcolor}{fg=white,bg=airforceblue}
    \begin{beamercolorbox}[wd=0.08\paperwidth,ht=2.5ex,dp=1.125ex]{footlcolor}
    \hspace{0.3em}\insertshortauthor\hspace{0.3em}
    \end{beamercolorbox}
    \begin{beamercolorbox}[wd=0.86\paperwidth,ht=2.5ex,dp=1.125ex]{headline}%
    \insertsectionnavigationhorizontal{0.86\paperwidth}{\bfseries\hfill}{\hfill}
    \end{beamercolorbox}
    \begin{beamercolorbox}[wd=0.06\paperwidth,ht=2.5ex,dp=1.125ex]{footlcolor}
    \hspace{0.3em}%
    \insertframenumber/\inserttotalframenumber
    \end{beamercolorbox}
  }
}
